<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/seo/categories/get" method="GET">
        <service class="Ibo\Sitemap\Api\CategoryRepositoryInterface" method="get"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
    <route url="/V1/seo/categories/update" method="POST">
        <service class="Ibo\Sitemap\Api\CategoryRepositoryInterface" method="update"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
    <route url="/V1/seo/categories/getbrand" method="GET">
        <service class="Ibo\Sitemap\Api\CategoryRepositoryInterface" method="getBrand"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
    <route url="/V1/seo/categories/update-brand" method="POST">
        <service class="Ibo\Sitemap\Api\CategoryRepositoryInterface" method="updateBrand"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
    
</routes>
