<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">   
    <preference for="Embitel\Catalog\Api\CategoryManagementInterface" type="Embitel\Catalog\Model\CategoryManagement" />
    <preference for="Embitel\Catalog\Api\BomOfferIdInterface" type="Embitel\Catalog\Model\BomOfferId" />
    <virtualType name="additionalFieldsProviderForElasticsearch" type="Magento\AdvancedSearch\Model\Adapter\DataMapper\AdditionalFieldsProvider">
        <arguments>
            <argument name="fieldsProviders" xsi:type="array">               
                <item name="allowedChannels" xsi:type="object">Embitel\Catalog\Model\Adapter\BatchDataMapper\AllowedChannelsDataProvider</item>
                <item name="serviceCategory" xsi:type="object">Embitel\Catalog\Model\Adapter\BatchDataMapper\ServiceCategoryDataProvider</item>
            </argument>
        </arguments>
    </virtualType>
    <preference for="Magento\MediaStorage\Service\ImageResize" type="Embitel\Catalog\Service\ImageResize" />
    <preference for="Embitel\Catalog\Api\IboCartInterface" type="Embitel\Catalog\Model\IboCart" />
</config>
