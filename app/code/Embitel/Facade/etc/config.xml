<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <facade>
            <general>
                <cron_status>0</cron_status>
                <batch_size>50</batch_size>
                <failure_hit_limit>3</failure_hit_limit>
                <cron_expression>0 0 * * *</cron_expression>
                <log_active>1</log_active>
            </general>
        </facade>
        <oodo_sync>
            <general>
                <cron_status>0</cron_status>
                <batch_size>50</batch_size>
                <failure_hit_limit>3</failure_hit_limit>
                <update_failure_hit_limit>3</update_failure_hit_limit>
                <cron_expression>0 0 * * *</cron_expression>
                <log_active>1</log_active>
            </general>
        </oodo_sync>
        <catalog_service>
            <general>
                <core_attribute_list>brand_color,ebo_color,pack_of</core_attribute_list>                
            </general>
        </catalog_service>
    </default>
</config>
