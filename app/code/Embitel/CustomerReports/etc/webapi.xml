<?xml version="1.0" ?> 
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd"> 
	<route method="GET" url="/V1/get-customers-report"> 
		<service class="Embitel\CustomerReports\Api\ReportManagementInterface" method="getCustomers"/> 
			<resources> 
				<resource ref="Magento_Customer::customer"/> 
			</resources> 
	</route> 
	<route method="GET" url="/V1/get-customers-orders"> 
		<service class="Embitel\CustomerReports\Api\ReportManagementInterface" method="getCustomerOrders"/> 
		<resources> 
			<resource ref="Magento_Customer::customer"/>
		</resources>
	</route>	
</routes>