<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Api/etc/extension_attributes.xsd">
     <extension_attributes for="Magento\Sales\Api\Data\OrderInterface">
        <attribute code="shipping_tax_rate" type="string"/>
        <attribute code="ept_option_title" type="mixed" />
        <attribute code="is_invoice" type="boolean" />
        <attribute code="is_shipment" type="boolean" />
        <attribute code="apply_discount_on_prices" type="string"/>
        <attribute code="apply_shipping_on_prices" type="string"/>
    </extension_attributes>
    <extension_attributes for="Magento\Catalog\Api\Data\EavAttributeInterface">
        <attribute code="import_custom" type="string"/>
    </extension_attributes>
	<extension_attributes for="Magento\Catalog\Api\Data\ProductInterface">
        <attribute code="website_ids" type="mixed"/>
        <attribute code="configurable_product_options_data" type="mixed"/>
        <attribute code="configurable_product_link_data" type="mixed"/>
        <attribute code="website_wise_product_price_data" type="mixed"/>
    </extension_attributes>
</config>