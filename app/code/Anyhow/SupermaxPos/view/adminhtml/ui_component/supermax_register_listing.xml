<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">supermax_register_listing.supermax_register_listing_data_source</item>
            <item name="deps" xsi:type="string">supermax_register_listing.supermax_register_listing_data_source</item>
        </item>
        <item name="buttons" xsi:type="array">
            <item name="export" xsi:type="array">
                <item name="name" xsi:type="string">export</item>
                <item name="label" xsi:type="string" translate="true">Export Registers</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">supermax/register/exportdata</item>
                <item name="sortOrder" xsi:type="number">30</item>
            </item>
        </item>
        <item name="spinner" xsi:type="string">register_columns</item>
    </argument>
	<dataSource name="supermax_register_listing_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider</argument>
            <argument name="name" xsi:type="string">supermax_register_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">pos_register_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
			<argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">pos_register_id</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
	<listingToolbar name="listing_top">
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls" />
		<filterSearch name="fulltext"/>
		<filters name="listing_filters">
            <argument name="data" xsi:type="array">
                <item name="observers" xsi:type="array">
                    <item name="column" xsi:type="string">column</item>
                </item>
            </argument>
            <settings>
                <templates>
                    <filters>
                        <select>
                            <param name="template" xsi:type="string">ui/grid/filters/elements/ui-select</param>
                            <param name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</param>
                        </select>
                    </filters>
                </templates>
            </settings>
        </filters>
		<paging name="listing_paging"/>
    </listingToolbar>
	<columns name="register_columns">
        <settings>
            <editorConfig>
                <param name="clientConfig" xsi:type="array">
                    <item name="saveUrl" xsi:type="url" path="supermax/register/update"/>
                    <item name="validateBeforeSave" xsi:type="boolean">false</item>
                </param>
                <param name="indexField" xsi:type="string">pos_register_id</param>
                <param name="enabled" xsi:type="boolean">true</param>
                <param name="selectProvider" xsi:type="string">supermax_register_listing.supermax_register_listing.register_columns.ids</param>
            </editorConfig>
            <childDefaults>
                <param name="fieldAction" xsi:type="array">
                    <item name="provider" xsi:type="string">supermax_register_listing.supermax_register_listing.register_columns_editor</item>
                    <item name="target" xsi:type="string">startEdit</item>
                    <item name="params" xsi:type="array">
                        <item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
                        <item name="1" xsi:type="boolean">true</item>
                    </item>
                </param>
            </childDefaults>
        </settings>
        <selectionsColumn name="ids">
            <settings>
                <indexField>pos_register_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>55</resizeDefaultWidth>
            </settings>
        </selectionsColumn>
        <column name="pos_register_id">
            <settings>
                <filter>textRange</filter>
                <label translate="true">ID</label>
                <sorting>desc</sorting>
            </settings>
        </column>
        <column name="date_open">
            <settings>
                <filter>dateRange</filter>
                <label translate="true">Opened At</label>
                <!-- <sorting>desc</sorting> -->
            </settings>
        </column>
        <column name="date_close">
            <settings>
                <filter>dateRange</filter>
                <label translate="true">Closed At</label>
            </settings>
        </column>
        <column name="reconciliation_status" class="Anyhow\SupermaxPos\Ui\Component\Listing\Columns\ReconcileStatus">
            <settings>
                <bodyTmpl>ui/grid/cells/html</bodyTmpl>
                <label translate="true">Reconciliation Status</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="name">
            <settings>
                <filter>text</filter>
                <label translate="true">Register Name</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="pos_outlet_id" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <options class="Anyhow\SupermaxPos\Model\Cashier\Attribute\Outlets\AllOutletOptions"/>
                <filter>select</filter>
                <dataType>select</dataType>
                <label translate="true">Store Name</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="pos_user_id" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <options class="Anyhow\SupermaxPos\Model\Cashier\Attribute\Outlets\AllCashierOptions"/>
                <filter>select</filter>
                <dataType>select</dataType>
                <label translate="true">Employee Name</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="status" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <options class="Magento\Config\Model\Config\Source\Enabledisable"/>
                <filter>select</filter>
                <dataType>select</dataType>
                <label translate="true">Status</label>
            </settings>
        </column>
        <column name="close_note">
            <settings>
                <label translate="true">Cashier Clouser Note</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="totalexpected" class="Anyhow\SupermaxPos\Ui\Component\Listing\Columns\TotalExpected">
            <settings>
                <label translate="true">Total Expected</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="floatamount" class="Anyhow\SupermaxPos\Ui\Component\Listing\Columns\FloatAmount">
            <settings>
                <label translate="true">Float Amount</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="totaldeclared" class="Anyhow\SupermaxPos\Ui\Component\Listing\Columns\TotalDeclared">
